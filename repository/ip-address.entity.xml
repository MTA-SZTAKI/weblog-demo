<?xml version="1.0" encoding="UTF-8"?>
<entity-package xmlns="urn:hu.sztaki.ilab.longneck:1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <!-- Entities of ip addresses. -->

  <!-- check ipaddress -->
  <entity id="ipaddress.check">
    <match apply-to="ipAddress" regexp="^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$" />
  </entity>
</entity-package>

